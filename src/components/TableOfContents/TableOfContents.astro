---
const content = Astro.props;
---

<nav class="toc" id="table-of-contents">
  <h2>Table of Contents</h2>
    <ul>
        {content.internalLinks.map((item) => (
            <li>
                <a href={`#${item.link}`} class="nav-item">{item.text}</a>
            </li>
        ))}
    </ul>
</nav>